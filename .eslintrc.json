{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "import"
    ],
    "rules": {
        "import/extensions": ["error", "ignorePackages", {
            "js": "never", // You can customize this based on your project's needs
            "jsx": "never"}],
        "quotes": ["error", "double", { "allowTemplateLiterals": true }]
    }
}
